{

  "compilerOptions": {
    "outDir": "build",
  "declaration": true,
    "target": "es2016",
    "module": "commonjs",
    "typeRoots": [
      "./src/@types",
      "./custom_typings",
      "./@types",
      "./node_modules/@types"
